apiVersion: 1

datasources:
  - name: Postgres
    type: postgres
    access: proxy
    url: 127.0.0.1:5432
    user: grafana
    editable: true
    secureJsonData:
        password: "{{ postgresql.users.grafana.password }}"
    jsonData:
        database: empowerd
        sslmode: 'disable'
        postgresVersion: 1300
    version: 1
